      }
    image 
      {
        function        = EXTENSION
	recolorable     = TRUE
	file            = "tab_bottom.png"
	border          = { 3, 3, 3, 3 }
	stretch         = TRUE
	gap_side        = TOP
      }
    image 
      {
        function        = EXTENSION
	recolorable     = TRUE
